{
  "name": "posts-navigator",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3000",
  "homepage": ".",
  "dependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/node": "7.10.5",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/register": "7.11.5",
    "@babel/runtime": "7.11.2",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@testing-library/user-event": "12.1.6",
    "axios": "0.20.0",
    "axios-mock-adapter": "1.18.2",
    "chai": "4.2.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "coveralls": "3.1.0",
    "debug": "4.2.0",
    "dotenv": "^8.2.0",
    "express": "4.16.1",
    "http-errors": "1.6.3",
    "mocha": "8.1.3",
    "morgan": "1.9.1",
    "node-sass": "4.14.1",
    "nodemon": "2.0.4",
    "normalize.css": "8.0.1",
    "nyc": "15.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.1",
    "react-scripts": "3.4.3",
    "redux": "4.0.5",
    "redux-mock-store": "1.5.4",
    "redux-saga": "^1.1.3",
    "redux-thunk": "2.3.0",
    "sinon-chai": "3.5.0",
    "supertest": "4.0.2"
  },
  "scripts": {
    "deploy": "gh-pages -d build",
    "start": "concurrently --kill-others-on-fail \"npm run server:start\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --watchAll --coverage --collectCoverageFrom=!src/serviceWorker.js",
    "eject": "react-scripts eject",
    "preserver:start": "babel ./server --out-dir ./server/build",
    "server:start": "node ./server/build/bin/www",
    "start-server-dev": "nodemon --exec babel-node ./server/bin/www",
    "lint": "./node_modules/.bin/eslint ./server",
    "pretty": "prettier --write **/*.{js,json} !node_modules/**",
    "postpretty": "npm run lint --fix",
    "server:test": "nyc --reporter=html --reporter=text --reporter=lcov mocha './server/test/**/*.test.js' -r @babel/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "app:start": "concurrently --kill-others-on-fail \"npm run start-server-dev\" \"react-scripts start\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "gh-pages": "3.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3"
  }
}
